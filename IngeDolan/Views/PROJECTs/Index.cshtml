@model IEnumerable<IngeDolan.Models.PROJECT>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.STARTING_DATE)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FINAL_DATE)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DESCRIPTIONS)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PROJECT_NAME)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.LEADER_ID)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.STARTING_DATE)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FINAL_DATE)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DESCRIPTIONS)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PROJECT_NAME)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.USER.USERNAME)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.PROJECT_ID }) |
            @Html.ActionLink("Details", "Details", new { id=item.PROJECT_ID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.PROJECT_ID })
        </td>
    </tr>
}

</table>
